{
    "name": "hanaboso/pipes-connectors",
    "description": "Connectors for PipesFramework",
    "version": "1.6.1",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "hanaboso/app-store": "^1.6",
        "hanaboso/pipes-php-sdk": "^1.6",
        "hanaboso/rabbit-mq-bundle": "^1.5"
    },
    "require-dev": {
        "aws/aws-sdk-php-symfony": "^2.2",
        "hanaboso/php-check-utils": "^1.3",
        "symfony/debug-bundle": "^v5.3",
        "symfony/security-bundle": "^v5.3",
        "symfony/serializer": "^v5.3"
    },
    "suggest": {
        "aws/aws-sdk-php-symfony": "Install me, if you need to use Model\\Application\\Impl\\AmazonApps."
    },
    "autoload": {
        "psr-4": {
            "Hanaboso\\HbPFConnectors\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HbPFConnectorsTests\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../pipes-php-sdk",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../app-store",
            "options": {
                "symlink": true
            }
        }
    ]
}
